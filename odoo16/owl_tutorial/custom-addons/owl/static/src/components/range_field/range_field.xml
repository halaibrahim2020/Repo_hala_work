<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-name="owl.RangeField" owl="1">
        <label for="customRange1" class="form-label"><span t-esc="props.value"/> <span t-esc="currency"/></label>
        <input type="range" class="form-range" id="customRange1" min="100" max="1000" step="100" t-on-change="(ev)=>this.props.update(ev.target.value)" t-att-value="props.value"/>
    </t>
</templates>